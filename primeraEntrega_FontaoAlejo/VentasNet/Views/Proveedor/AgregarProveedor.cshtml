@using VentasNet.Infra.DTO.Request;
@model ProveedorRequest
<div class="card-body">
	<form asp-action="AgregarProveedor" method="post">
		<div class="form-group">
			<label asp-for="RazonSocial"></label>
			<input asp-for="RazonSocial" class="form-control" />
		</div>
		<div class="form-group">
			<label asp-for="Cuit"></label>
			<input asp-for="Cuit" class="form-control" />
		</div>
		<div class="form-group">
			<label asp-for="Domicilio"></label>
			<input asp-for="Domicilio" class="form-control" />
		</div>
		<div class="form-group">
			<label asp-for="Provincia"></label>
			<input asp-for="Provincia" class="form-control" />
		</div>
		<button type="submit" class="btn btn-primary">Agregar</button>
	</form>
</div>